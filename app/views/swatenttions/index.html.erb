<div class='box'>        
<div class='box-content'>
    <!-- FORM ROW -->
    <div class='row-fluid' onload="loadFormScripts()" >
      <%= render 'form' %>
    </div>
    
    <!-- RECORDS ROW -->
    <div class='row-fluid'>
      <div class="span12">
          <hr class='title-line'> 
          <% if @user_attentions.blank? == false %>         
            <%= render 'sendreport' %>
          <% end %>
      </div>
    </div>

    <div class='row-fluid'>  
      <div class="span12 out-margin ">
        <div class="responsive-table records-size">
          <div class='scrollable-area' style="height:400px">
            <%= render 'swatenttionslist' %>
          </div> 
        </div>
      </div>
    </div>
</div>
</div> 

